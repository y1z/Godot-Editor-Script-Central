; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="proj"
run/main_scene="uid://dh0e0uaefb3q3"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[debug]

gdscript/warnings/shadowed_variable=2
gdscript/warnings/shadowed_variable_base_class=2
gdscript/warnings/shadowed_global_identifier=2
gdscript/warnings/missing_tool=2

[display]

window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Entities/": "orange",
"res://Scenes/": "red",
"res://Scripts/": "green",
"res://Sprites/": "yellow",
"res://Textures/": "purple",
"res://Themes/": "pink"
}

[input_devices]

sensors/enable_gravity=true
sensors/enable_gyroscope=true

[rendering]

textures/canvas_textures/default_texture_filter="Nearest"
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
